            <if test="fdReporImpact != null  ">Fd_Repor_Impact = #{fdReporImpact},</if>
            <if test="fdReporImage != null  and fdReporImage != ''  ">Fd_Repor_image = #{fdReporImage},</if>
            <if test="fdCreatetime != null  ">Fd_createtime = #{fdCreatetime},</if>
