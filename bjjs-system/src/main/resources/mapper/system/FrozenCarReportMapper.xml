    <!--查询喷洒冻车数-->
    <sql id="selectSprayingByOperationCounts">
SELECT
	f.Fd_Report_id,
	f.Fd_createtime,
	f.Fd_Estimate_Relieve_datetime,
	f.Fd_manual_Relieve_datetime,
	f.Fd_Relieve,
	f.Fd_remarks,
	f.Fd_Repor_count,
	f.Fd_Repor_datetime,
	f.Fd_Repor_dept_id,
	f.Fd_Repor_djhd,
	f.Fd_Repor_dz,
	f.Fd_Repor_fz,
	f.Fd_Repor_Impact,
	f.Fd_Repor_License,
	f.Fd_Spray,
	f.Fd_Repor_name,
	f.Fd_Repor_No,
	f.Fd_Repor_num,
	f.Fd_verify,
	s.Spray_datetime
FROM
	Frozen_car_Report f
INNER JOIN Spraying_operation s ON s.Freezer_id = f.Fd_Report_id
	</sql>
